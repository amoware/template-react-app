{
  "name": "template-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "run-s lint prettier && vitest run",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react": "18.0.22",
    "@types/react-dom": "18.0.7",
    "@vitejs/plugin-react": "2.2.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-vitest-globals": "1.2.0",
    "husky": "8.0.1",
    "jsdom": "20.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "vite": "3.2.0",
    "vite-plugin-zip-pack": "1.0.4",
    "vitest": "0.24.5"
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint",
    "*.{js,jsx,css,md,html,json}": "npm run prettier"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
